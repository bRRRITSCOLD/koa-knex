$ const products = data.products;

<!doctype html>
<html lang="en">
    <head>
        <meta charset="UTF-8"/>
        <title>Products</title>
    </head>
    <body>
      <div>
        <ul for(product in products)>
          <li>Name: ${product.name}</li>
          <li>Cost: ${product.cost}</li>
          <li>Price: ${product.price}</li>
          <li>Category: ${product.category}</li>
        </ul>
      </div>
    </body>
</html>
