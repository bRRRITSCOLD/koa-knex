$ const timestamp = data.timestamp[0];

<!doctype html>
<html lang="en">
    <head>
        <meta charset="UTF-8"/>
        <title>Customer Timestamps</title>
    </head>
    <body>
      <div>
        <ul>
          <li>Customer ID: ${timestamp.customer_id}</li>
          <li>Created on: ${timestamp.created_at}</li>
          <li>Last Updated: ${timestamp.updated_at}</li>
        </ul>
      </div>
    </body>
</html>
